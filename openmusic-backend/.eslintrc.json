{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "eol-last": "off",
        "no-unused-vars": "off",
        "class-methods-use-this": "off",
        "object-curly-newline": "off",
        "camelcase": "off",
        "no-trailing-spaces": "off",
        "no-console": "off",
        "newline-per-chained-call": "off",
        "quotes": "off",
        "max-len": "off"
    }
}
